{
    "name": "tjura/yii2-migration-helper",
    "description": "Simple interactive migration helper for Yii2 framework",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomasz Jura",
            "email": "jura.tomasz@gmail.com",
            "homepage": "https://github.com/tjura",
            "role": "Creator"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "~7.0",
        "yiisoft/yii2": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "tjura\\": "app/"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
